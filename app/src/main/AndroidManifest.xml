<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ECardapio"
        tools:targetApi="31">
        <activity
            android:name=".view.login.administrator.AdministratorLoginActivity"
            android:exported="false"
            android:label="@string/title_activity_administrator"
            android:theme="@style/Theme.ECardapio" />
        <activity
            android:name=".view.login.collaborator.CollaboratorLoginActivity"
            android:exported="false"
            android:label="@string/title_activity_collaborator_login"
            android:theme="@style/Theme.ECardapio" />
        <activity
            android:name=".view.login.register.RegisterUserActivity"
            android:exported="false"
            android:label="@string/title_activity_register_user"
            android:theme="@style/Theme.ECardapio" />
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:theme="@style/Theme.ECardapio">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>